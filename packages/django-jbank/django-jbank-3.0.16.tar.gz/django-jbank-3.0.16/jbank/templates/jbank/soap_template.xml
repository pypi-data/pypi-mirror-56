<?xml version="1.0" encoding="UTF-8"?>
<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:cor="http://bxd.fi/CorporateFileService" xmlns:mod="http://model.bxd.fi">
    <soapenv:Header />
    <soapenv:Body>
        <cor:{{ soap_call.command_camelcase }}in>
            <mod:RequestHeader>
                <mod:SenderId>{{ soap_call.connection.sender_identifier }}</mod:SenderId>
                <mod:RequestId>{{ soap_call.request_identifier }}</mod:RequestId>
                <mod:Timestamp>{{ soap_call.timestamp.isoformat }}</mod:Timestamp>
                <mod:Language>FI</mod:Language>
                <mod:UserAgent>Kajala WS</mod:UserAgent>
                <mod:ReceiverId>{{ soap_call.connection.receiver_identifier }}</mod:ReceiverId>
            </mod:RequestHeader>
            <mod:ApplicationRequest>{{ payload }}</mod:ApplicationRequest>
        </cor:{{ soap_call.command_camelcase }}in>
    </soapenv:Body>
</soapenv:Envelope>
