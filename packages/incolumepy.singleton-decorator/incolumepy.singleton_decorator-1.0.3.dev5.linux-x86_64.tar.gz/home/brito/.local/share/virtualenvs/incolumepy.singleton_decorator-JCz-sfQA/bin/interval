#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'incolumepy.singleton-decorator==1.0.3.dev5','console_scripts','interval'
__requires__ = 'incolumepy.singleton-decorator==1.0.3.dev5'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('incolumepy.singleton-decorator==1.0.3.dev5', 'console_scripts', 'interval')()
    )
