option_settings:
  aws:elasticbeanstalk:cloudwatch:logs:
    StreamLogs: 'true'
    DeleteOnTerminate: 'true'
    RetentionInDays: '30'
  aws:elasticbeanstalk:cloudwatch:logs:health:
    HealthStreamingEnabled: 'true'
    DeleteOnTerminate: 'true'
    RetentionInDays: '30'
  aws:elasticbeanstalk:hostmanager:
    LogPublicationControl: 'true'
