<div>
    <div class="row">
        <div class="col-md-12">
            {{#if learners}}
                <div class="top-scroll"><div class="scroller"></div></div>
                <div id="displaygrid">
                    <div class="users-table">
                        <table class="table table-bordered tab_report">
                            <thead>
                                <tr>
                                    <th class="headrowuser">
                                        {{_ "Learner"}}
                                    </th>
                                    {{#each contents}}
                                         <th class="headrow data">
                                            <div>
                                                <a href="{{ url "learn" }}{{ path }}" title='"{{ display_name }}"{{#if description }} {{ description }}{{/if }}'>{{ title }}</a>
                                            </div>
                                        </th>
                                    {{/each}}
                                </tr>
                            </thead>
                            <tbody class="student-data">
                            </tbody>
                        </table>
                        <div style="clear: both;">
                        </div>
                    </div>
                </div>
            {{/if}}
        </div>
    </div>
</div>
