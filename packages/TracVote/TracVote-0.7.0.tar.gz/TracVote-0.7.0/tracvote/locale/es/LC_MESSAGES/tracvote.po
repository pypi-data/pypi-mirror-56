#: tracvote/__init__.py:194
msgid "List of URL paths to allow voting on. Globs are supported."
msgstr "Lista de URLs que permiten votar. Se aceptan globs."

#: tracvote/__init__.py:402
msgid "Secure cookies are enabled, you must use https for your requests."
msgstr "Estan activadas las cookies seguras, el uso de httpS es un requisito. "

#: tracvote/__init__.py:405
msgid "Do you have cookies enabled?"
msgstr "¿Has habilitado el uso de cookies?"

#: tracvote/__init__.py:485
msgid "Show most recently voted resources."
msgstr "Muesta los recursos que han sido votados recientemente."

#: tracvote/__init__.py:487
msgid "Show listing of voted resources ordered by total score."
msgstr "Ver la lista de recursos votados ordenados por puntos totales."

#: tracvote/__init__.py:489
msgid "Show listing of most recent votes for a resource."
msgstr "Ver lista de vostos recientes por un recurso determinado."

#: tracvote/__init__.py:515
#, python-format
msgid "by %(author)s at %(time)s"
msgstr "por %(author)s en %(time)s"

#: tracvote/__init__.py:547
#, python-format
msgid "at %(date)s"
msgstr "en fecha %(date)s"

#: tracvote/__init__.py:551
#, python-format
msgid "%(count)s by %(author)s %(vote)s"
msgstr "%(count)s de %(vote)s por %(author)s"

#: tracvote/__init__.py:567
msgid ""
"A newer version of VotePlugin has been installed before, but downgrading "
"is unsupported."
msgstr ""
"Una versión más nueva de VotePlugin ya se encuentra instalada, no es "
"posible instalar una anterior."

#: tracvote/__init__.py:602
#, python-format
msgid "No upgrade module for version %(num)i (%(version)s.py)"
msgstr "No hay actualizaciones para la version %(num)i (%(version)s.py) del módulo"

#: tracvote/__init__.py:646 tracvote/__init__.py:657
msgid "Up-vote"
msgstr "Voto afirmativo"

#: tracvote/__init__.py:648 tracvote/__init__.py:654
msgid "Down-vote"
msgstr "Voto negativo"

#: tracvote/__init__.py:661
msgid ""
"You can vote for resources on this Trac install by clicking the up-vote"
"/down-vote arrows in the context navigation bar."
msgstr ""
"Puedes votar por recursos a instalar en este Trac dando click en las "
"flechas «Voto por sí/Voto por no» en la barrra de navegación contextual."

#: tracvote/__init__.py:682
#, python-format
msgid "Vote count%(detail)s"
msgstr "Detalles de escrutinio de%(detail)s"

#: tracvote/__init__.py:702
#, python-format
msgid "Unsupported database type \"%s\""
msgstr "La base de datos \"%s\" no está soportada."

