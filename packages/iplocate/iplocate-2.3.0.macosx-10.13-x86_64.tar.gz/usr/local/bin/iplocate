#!/usr/local/opt/python@2/bin/python2.7
# EASY-INSTALL-ENTRY-SCRIPT: 'iplocate==2.3.0','console_scripts','iplocate'
__requires__ = 'iplocate==2.3.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('iplocate==2.3.0', 'console_scripts', 'iplocate')()
    )
