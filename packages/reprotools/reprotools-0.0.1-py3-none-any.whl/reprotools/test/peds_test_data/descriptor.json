    "container-image": {
        "image": "nurm_centos7_latest", 
        "type": "docker"
    }, 
    "description": "Test Script for auto peds", 
    "inputs": [
        {
            "description": "input files", 
            "id": "input_file", 
            "name": "input file", 
            "optional": false, 
