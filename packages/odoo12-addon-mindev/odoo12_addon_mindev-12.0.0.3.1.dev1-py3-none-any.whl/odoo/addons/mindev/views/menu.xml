<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="edit_menu_access" model="ir.ui.view">
            <field name="name">ir.ui.menu.mindev.form</field>
            <field name="model">ir.ui.menu</field>
            <field name="inherit_id" ref="base.edit_menu_access"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="XML" groups="mindev.group_mindev">
                        <group>
                            <field name="xml_id" readonly="1"/>
                            <field name="xml_module" readonly="1"/>
                            <field name="xml_name" readonly="1"/>
                        </group>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
