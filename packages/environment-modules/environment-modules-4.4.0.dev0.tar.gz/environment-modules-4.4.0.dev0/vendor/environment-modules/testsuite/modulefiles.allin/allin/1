chdir $env(HOME)
setenv ts1 ok
getenv ts1
unsetenv ts2
unsetenv ts2 $env(ts1)
setenv tsc $env(ts1)
unsetenv tsc
setenv tsu $env(tsc)
append-path -d " " TSPATH dir1
append-path -d " " TSPATH dir2
prepend-path -d " " TSPATH dir3
remove-path -d " " TSPATH dir1
setenv tsp $env(TSPATH)
is-loaded allin
is-saved allin
is-used allin
is-avail allin
module-info type
module-info mode
module-info command
module-info name
module-info specified
module-info shell
module-info shelltype
module-info alias allin
module-info version allin
module-info symbols allin
module-info loaded allin

module aliases
module avail -t allin
module display allin/5
module list -t
module save
module restore
module savelist -t
module saveshow
module saverm
module load allin/5
module source ./5
module test allin/5
module unload allin/4
module use -a /path/to/dir
module unuse /path/to/dir

module-version /3 3.0
module-alias foo allin/1
module-virtual /6 5
module-whatis allin
set-alias foo bar
unset-alias bar
system echo foo
uname sysname
uname nodename
uname domain
uname release
uname version
uname machine
x-resource {Ileaf.popup.saveUnder:True}
